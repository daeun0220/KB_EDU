package com.edu.array2.test;

public class ArrayTest1 {

	public static void main(String[] args) {
		// 배열 선언 + 생성 + 초기화 
		int[] arr1 = {11, 22, 33};
		int[] arr2 = {100, 200, 300};
		
		// 배열 안에 있는 값들을 반복문을 통해서 출력
		for(int i : arr1) System.out.println(i);
		for(int j : arr2) System.out.println(j);
		
		System.out.println("==================");
		
		arr1 = arr2;
		
		for(int i : arr1) System.out.println(i);
		for(int j : arr2) System.out.println(j);
		

	}
}